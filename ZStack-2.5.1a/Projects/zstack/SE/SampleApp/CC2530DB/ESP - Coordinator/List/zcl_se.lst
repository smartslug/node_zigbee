   6994                  break;
   6995          
   6996                case COMMAND_SE_GET_SPECIAL_DAYS:
   6997                  stat = zclSE_ProcessInCmd_Tou_GetSpecialDays( pInMsg, pCBs );
   6998                  break;
   6999          
   7000                default:
